#include<stdio.h>
void main()
{
int m,n;
printf("Enter the value of m,n: \n");
scanf("%d%d",&m,&n);
while(m<=n)
{if(m%2==0)
    {printf("%d \t ",m);}
m++;
}
}

#include<stdio.h>
void main()
{
int m,n,sum=0;
printf("Enter the number:\n");
scanf("%d",&n);
while(n>0)
{m=n%10;
sum=sum+m;
n/=10;}
printf("Sum= %d",sum);
}

#include<stdio.h>
void main()
{
int i,n;
i=1;
printf("Enter the number whose multiples are to be found: \n");
scanf("%d",&n);
while(i<=100)
{
if(i%n==0)
{printf("%d \t",i);}
i++;
}
}
